<%- include('../../../partials/header-details.ejs') %> 
<%- include('../../../components/navbar') %>

<div class="container-fluid">
  <div class="row">
<%- include('../../../components/sidebar') %>
    

    <main role="main" class="col-md-9 ml-sm-auto col-lg-10 px-4">
      <div
        class="
          d-flex
          justify-content-between
          flex-wrap flex-md-nowrap
          align-items-center
          pt-3
          pb-2
          mb-3
          border-bottom
        "
      >
        <h1 class="h2">Overview</h1>
      </div>
      <div class="card">
        <div class="card-body">
          <p>Issue Tracking</p>
          <ul>
            <li>Bug: <%= uncompletedBug1 %> Open / <%= totalBug1 %></li>
            <li>Feature: <%= uncompletedFeature1 %> Open / <%= totalFeature1 %></li>
            <li>Support: <%= uncompletedSupport1 %> Open / <%= totalSupport1 %></li>
          </ul>
        </div>
      </div>
      <div class="card card-members-styling">
        <div class="card-body">
          <p>Members</p>
          <ul>
            <% data.forEach(item => { %> 
            <li><%= item.firstname %> <%= item.lastname %> </li>
            <% }) %> 
          </ul>
        </div>
      </div>
    </main>
  </div>
</div>
<%- include('../../../partials/footer-details.ejs') %>
