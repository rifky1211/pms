  <br>
  <h1>Projects</h1>
  <br>
  <h3>Filter</h3>
  <form class="form-horizontal" action="" method="get">
    <input type="hidden" name="page" value="1">
    
    <div class="form-group">
      <input type="checkbox" id="checkString" onclick="myFunction1()" name="checkString" />
      <label class="control-label col-sm-2" for="string">ID :</label>
      <div class="col-sm-4">
        <input
          type="number"
          class="form-control"
          id="id"
          name="id"
          placeholder="ID"
        
        />
      </div>
    </div>
    <div class="form-group">
      <input type="checkbox" id="checkAngka" onclick="myFunction2()" >
      <label class="control-label col-sm-2" for="integer">Name :</label>
      <div class="col-sm-4">
        <input
          type="text"
          class="form-control"
          id="name"
          name="name"
          placeholder="Enter Integer..."
        
        />
      </div>
    </div>
    <div class="form-group">
      <input type="checkbox" id="checkBoolean" onclick="myFunction5()">
      <label for="member" class="control-label col-sm-2">Member :</label>
      <div class="col-sm-2">
        <select name="member" id="member" class="form-select col-sm-10">
          <option value="">Find Member ... </option>
          <option value="true">True</option>
          <option value="false">False</option>
        </select>
      </div>
    </div>
    <div class="form-group">
      <div class="col-sm-offset-2 col-sm-10">
        <button type="submit" class="btn btn-primary btn-search">Find</button>
        <a href="/" class="btn btn-warning btn-reset">Reset</a>
      </div>
    </div>
  </form>
<%- include('./partials/footer') %>
